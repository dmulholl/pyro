assert !$is_stack("foobar");

var foo = $stack();
assert $is_stack(foo);
assert foo:count() == 0;
assert foo:is_empty();
assert $is_err(foo:pop());
assert $is_err(foo:peek());

foo:push("abc");
assert foo:peek() == "abc";
foo:push("def");
assert foo:peek() == "def";

assert foo:count() == 2;
assert !foo:is_empty();

assert foo:pop() == "def";
assert foo:pop() == "abc";
assert foo:count() == 0;
assert foo:is_empty();

var bar = $stack();
for i in $range(5) {
    bar:push(i);
}

var index = 0;
for item in bar {
    assert item == index;
    index += 1;
}

def $test_clear() {
    var stack = $stack();
    assert stack:count() == 0;

    stack:push(123);
    stack:push(456);
    assert stack:count() == 2;

    stack:clear();
    assert stack:count() == 0;

    stack:push("A");
    stack:push("B");
    assert stack:pop() == "B";
    assert stack:pop() == "A";
}
