assert !$is_queue("foobar");

var foo = $queue();
assert $is_queue(foo);
assert foo:count() == 0;
assert foo:is_empty();
assert $is_err(foo:dequeue());
assert $is_err(foo:peek());

foo:enqueue("abc");
assert foo:peek() == "abc";

foo:enqueue("def");
assert foo:peek() == "abc";

foo:enqueue("ghi");
assert foo:peek() == "abc";

assert foo:count() == 3;
assert !foo:is_empty();

assert foo:dequeue() == "abc";
assert foo:dequeue() == "def";
assert foo:dequeue() == "ghi";

assert foo:count() == 0;
assert foo:is_empty();

for i in $range(100) {
    foo:enqueue(i);
}

assert foo:count() == 100;

for i in $range(100) {
    assert foo:dequeue() == i;
}

assert foo:count() == 0;
assert foo:is_empty();

def $test_clear() {
    var queue = $queue();
    assert queue:count() == 0;

    queue:enqueue("foo");
    queue:enqueue("bar");
    assert queue:count() == 2;

    queue:clear();
    assert queue:count() == 0;

    queue:enqueue(1);
    queue:enqueue(2);
    assert queue:dequeue() == 1;
    assert queue:dequeue() == 2;
}
