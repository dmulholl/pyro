import std::constants;

assert $is_f64(constants::pi);
assert constants::pi > 3.14 && constants::pi < 3.15;

assert $is_f64(constants::tau);
assert $is_f64(constants::e);
assert $is_nan(constants::nan);
assert $is_inf(constants::inf);

assert $is_i64(constants::i64_max);
assert $is_i64(constants::i64_min);

