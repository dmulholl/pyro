# Integers.
assert 0 < 0 == false;
assert 0 < 1 == true;
assert 1 < 0 == false;

assert 0 <= 0 == true;
assert 0 <= 1 == true;
assert 1 <= 0 == false;

assert 0 > 0 == false;
assert 0 > 1 == false;
assert 1 > 0 == true;

assert 0 >= 0 == true;
assert 0 >= 1 == false;
assert 1 >= 0 == true;

# Floats.
assert 0.0 < 0.0 == false;
assert 0.0 < 1.0 == true;
assert 1.0 < 0.0 == false;

assert 0.0 <= 0.0 == true;
assert 0.0 <= 1.0 == true;
assert 1.0 <= 0.0 == false;

assert 0.0 > 0.0 == false;
assert 0.0 > 1.0 == false;
assert 1.0 > 0.0 == true;

assert 0.0 >= 0.0 == true;
assert 0.0 >= 1.0 == false;
assert 1.0 >= 0.0 == true;

# Charsacters.
assert 'a' < 'a' == false;
assert 'a' < 'b' == true;
assert 'b' < 'a' == false;

assert 'a' <= 'a' == true;
assert 'a' <= 'b' == true;
assert 'b' <= 'a' == false;

assert 'a' > 'a' == false;
assert 'a' > 'b' == false;
assert 'b' > 'a' == true;

assert 'a' >= 'a' == true;
assert 'a' >= 'b' == false;
assert 'b' >= 'a' == true;
