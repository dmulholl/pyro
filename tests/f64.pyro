import $std::math;

assert $is_f64(1.0);
assert !$is_f64(1);
assert !$is_f64("foo");

assert $is_f64($f64(1.0));
assert $f64(1.0) == 1.0;

assert $f64(1) == 1.0;
assert $f64(-1) == -1.0;

assert $f64(4294967296) == 4294967296.0;

assert $is_f64(math::NAN);
assert $is_nan(math::NAN);

assert $is_f64(math::INF);
assert $is_inf(math::INF);
assert $is_inf(-math::INF);

assert $f64("0") == 0.0;
assert $f64("1") == 1.0;
assert $f64("123") == 123.0;
assert $f64("123.456") == 123.456;
assert $f64("1_234.567") == 1234.567;

assert $f64("1e3") == 1000.0;
assert $f64("1e6") == 1000000.0;
assert $f64("1e-3") == 0.001;
