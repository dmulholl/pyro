class Foo {
    pub var bar;
    pub var baz;
}

var foo = Foo();
var fields = $fields(foo):to_vec();
assert fields:count() == 2;
assert fields[0] == "bar";
assert fields[1] == "baz";


class Bar {
}

var bar = Bar();
var fields = $fields(bar):to_vec();
assert fields:is_empty();


assert $fields(123):to_vec():is_empty();
