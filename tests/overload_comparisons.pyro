class Foo {
    pub var value;

    def $init(value) {
        self.value = value;
    }

    def $op_binary_less(other) {
        return self.value < other.value;
    }

    def $op_binary_less_equals(other) {
        return self.value <= other.value;
    }

    def $op_binary_greater(other) {
        return self.value > other.value;
    }

    def $op_binary_greater_equals(other) {
        return self.value >= other.value;
    }
}

assert Foo(123) < Foo(456);

assert Foo(123) <= Foo(123);
assert Foo(123) <= Foo(456);

assert Foo(456) > Foo(123);

assert Foo(456) >= Foo(456);
assert Foo(456) >= Foo(123);
