class A {
    pub def foo() {
        return "a-foo";
    }
}


class B < A {
    pub def foo() {
        return "b-foo";
    }

    pub def get_closure() {
        return super:foo;
    }
}


class C < B {
}


var b = B();
var b_closure = b:get_closure();
assert b_closure() == "a-foo";


var c = C();
var c_closure = c:get_closure();
assert c_closure() == "a-foo";
