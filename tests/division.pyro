# Float division.
assert 10 / 5 == 2.0;
assert 10.0 / 5.0 == 2.0;
assert 10.0 / 5 == 2.0;
assert 10 / 5.0 == 2.0;

assert 11 / 5 == 2.2;
assert 11.0 / 5.0 == 2.2;
assert 11.0 / 5 == 2.2;
assert 11 / 5.0 == 2.2;

# Truncated division.
assert 10 // 5 == 2;
assert 10.0 // 5.0 == 2.0;
assert 10.0 // 5 == 2.0;
assert 10 // 5.0 == 2.0;

assert 11 // 5 == 2;
assert 11.0 // 5.0 == 2.0;
assert 11.0 // 5 == 2.0;
assert 11 // 5.0 == 2.0;
