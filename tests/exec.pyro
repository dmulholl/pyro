var globals = $exec("");
assert $is_map(globals);
assert globals:count() == 0;

var globals = $exec("var foo = 1 + 2 + 3;");
assert $is_map(globals);
assert globals:count() == 1;
assert globals["foo"] == 6;

var globals = $exec("var foo, bar; foo = $str(123); bar = 'a' + 'b';");
assert $is_map(globals);
assert globals:count() == 2;
assert globals["foo"] == "123";
assert globals["bar"] == "ab";
