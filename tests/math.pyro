import $std::math;

assert math::abs(1) == 1;
assert math::abs(-1) == 1;

assert math::abs(1.0) == 1.0;
assert math::abs(-1.0) == 1.0;

assert math::abs(1, 0) == 1;
assert math::abs(1.0, 0.0) == 1.0;
assert math::abs(-1, 0) == 1;
assert math::abs(-1.0, 0.0) == 1.0;

assert math::abs(math::i64_min, 0) == 0;
assert math::abs(math::i64_min, "foo") == "foo";
