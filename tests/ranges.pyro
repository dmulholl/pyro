var sum1 = 0;

for i in $range(5) {
    sum1 += i;
}

assert sum1 == 0 + 1 + 2 + 3 + 4;


var sum2 = 0;

for i in $range(5, 10) {
    sum2 += i;
}

assert sum2 == 5 + 6 + 7 + 8 + 9;


var sum3 = 0;

for i in $range(5, 10, 2) {
    sum3 += i;
}

assert sum3 == 5 + 7 + 9;


var sum4 = 0;

for i in $range(5, 0, -2) {
    sum4 += i;
}

assert sum4 == 5 + 3 + 1;
