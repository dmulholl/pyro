assert $num_compare(123, 456) == -1;
assert $num_compare(123, 123) == 0;
assert $num_compare(456, 123) == 1;

assert $num_compare(123.0, 456.0) == -1;
assert $num_compare(123.0, 123.0) == 0;
assert $num_compare(456.0, 123.0) == 1;

assert $num_compare('a', 'b') == -1;
assert $num_compare('a', 'a') == 0;
assert $num_compare('b', 'a') == 1;

assert $num_compare(1, 2.0) == -1;
assert $num_compare(1, 2.5) == -1;
assert $num_compare(0, 2.5) == -1;
assert $num_compare(5, 5.5) == -1;
assert $num_compare(-1, 1.0) == -1;
assert $num_compare(-2, -1.5) == -1;
assert $num_compare(-5, -1.5) == -1;
assert $num_compare(-6, -5.5) == -1;
assert $num_compare(1.0, 2) == -1;
assert $num_compare(1.5, 2) == -1;
assert $num_compare(0.0, 2) == -1;
assert $num_compare(5.0, 6) == -1;
assert $num_compare(5.5, 6) == -1;
assert $num_compare(-1.0, 1) == -1;
assert $num_compare(-2.0, -1) == -1;
assert $num_compare(-1.5, -1) == -1;
assert $num_compare(-5.5, -1) == -1;

assert $num_compare(0, 0.0) == 0;
assert $num_compare(1, 1.0) == 0;
assert $num_compare(-1, -1.0) == 0;
assert $num_compare(0.0, 0) == 0;
assert $num_compare(1.0, 1) == 0;
assert $num_compare(-1.0, -1) == 0;

assert $num_compare(1, 0.0) == 1;
assert $num_compare(1, 0.5) == 1;
assert $num_compare(2, 1.5) == 1;
assert $num_compare(2, -1.5) == 1;
assert $num_compare(-2, -2.5) == 1;
assert $num_compare(-1, -2.5) == 1;
assert $num_compare(1.0, 0) == 1;
assert $num_compare(1.5, 1) == 1;
assert $num_compare(2.0, 1) == 1;
assert $num_compare(2.5, 1) == 1;
assert $num_compare(2.5, -2) == 1;
assert $num_compare(-1.0, -2) == 1;
assert $num_compare(-0.5, -2) == 1;
assert $num_compare(-5.5, -6) == 1;

assert $num_compare($std::math::inf, 123) == 1;
assert $num_compare(-$std::math::inf, 123) == -1;

assert $num_compare(2.0 ** 65, 123) == 1;
assert $num_compare(-(2.0 ** 65), 123) == -1;

assert $num_eq(123, 123.0);
assert !$num_eq(123, 123.1);

assert $num_lt(123, 456.0);
assert !$num_lt(456.0, 123);

assert $num_gt(456, 123.0);
assert !$num_gt(123.0, 456);

assert $num_ge(123, 123.0);
assert $num_ge(456, 123.0);
assert !$num_ge(123, 456.0);

assert $num_le(123, 123.0);
assert $num_le(123, 456.0);
assert !$num_le(456.0, 123);
