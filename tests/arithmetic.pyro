# Integers
assert 1 + 2 == 3;
assert 1 * 2 == 2;
assert 2 + 3 * 4 == 14;
assert 2 * 3 + 4 == 10;
assert (2 + 3) * 4 == 20;

# Floats
assert 1.0 + 2.0 == 3.0;
assert 1.0 * 2.0 == 2.0;
assert 2.0 + 3.0 * 4.0 == 14.0;
assert 2.0 * 3.0 + 4.0 == 10.0;
assert (2.0 + 3.0) * 4.0 == 20.0;

# Mixed
assert 1 + 1.0 == 2.0;
assert 1.0 + 1 == 2.0;
assert 1 * 2.0 == 2.0;
assert 1 + 2 * 3.0 == 7.0;
