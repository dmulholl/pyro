def $test_join() {
    assert ", ":join(["foo", "bar", "baz"]) == "foo, bar, baz";
    assert "":join(["foo", "bar", "baz"]) == "foobarbaz";
    assert ", ":join(["foo"]) == "foo";
    assert ", ":join([]) == "";
}
